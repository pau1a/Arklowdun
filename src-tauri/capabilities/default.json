{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:path:default",
    "core:window:default",
    "core:webview:default",
    "dialog:default",
    "fs:default",
    "fs:scope-applocaldata-recursive",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "notification:default",
    "opener:default"
  ]
}
